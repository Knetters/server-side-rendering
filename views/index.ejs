                                  <tr>
                                    <th>Pass</th>
                                    <th>Turn</th>
                                    <th>O/D</th>
                                  </tr>
                                </thead>
                                <tbody>
                                    
                                    <%# Point item team 1 %>
                                    <tr class="score-info-team-1 green">
                                        <td>3</td>
                                        <td>0</td>
                                        <td>O</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="score-points">
