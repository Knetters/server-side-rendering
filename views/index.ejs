                                <thead>
                                  <tr>
                                    <th>Score</th>
                                  </tr>
                                </thead>
                                <tbody>

                                    <%# Score %>
                                    <tr>
                                        <td>1 - 0</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="points-team-2">
